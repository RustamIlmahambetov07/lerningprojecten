#include <stdio.h>

int main() {
  int i = 0;
  while (i < 5) {
    printf("%d\n", i);
    i++;
  }
  return 0;
}

// example 

#include <stdio.h>

int main() {

    int i = 0;

    do {
        printf("%d\n", i);
        i++;
    }
    while (i < 5);

}

// example

#include <stdio.h>

int main() {

    int i = 6;

    do {
        printf("%d\n", i);
        i++;
    }
    while (i < 5);

}
