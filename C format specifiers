#include <stdio.h>

int main() {

    int myOld = 17;
    float myFloatNum = '5,99';
    char myName = 'R';

    printf("My old is %d and my name is %c, My float name is %f", myOld, myName, myFloatNum);
}

// second 

#include <stdio.h>

int main (){

    int myNum = 5;
    float myFloatNum = 5.99;
    char myLetter = 'D';

    printf("%d\n", myNum);
    printf("%f\n", myFloatNum);
    printf("%c\n", myLetter);
}
